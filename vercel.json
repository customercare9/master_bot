{
  "version": 2,
  "framework": null,
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "installCommand": "pip install -r requirements.txt",
  "devCommand": "python main.py",
  "regions": ["iad1"],
  "functions": {
    "api/**/*.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ]
}
